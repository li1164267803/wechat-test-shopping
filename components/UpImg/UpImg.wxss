.up_img_wrap_box {
    display: flex;
}

.up_img_wrap {
    width: 90rpx;
    height: 90rpx;
    position: relative;
    margin-left: 20rpx;
    margin-top: 20rpx;
}

.up_img_wrap image {
    width: 100%;
    height: 100%;
    border-radius: 15rpx;
}

.up_img_wrap icon {
    position: absolute;
    top: -22rpx;
    right: -22rpx;
}